<p id="notice"><%= notice %></p>

<h1>Listing Myriads</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myriads.each do |myriad| %>
      <tr>
        <td><%= link_to 'Show', myriad %></td>
        <td><%= link_to 'Edit', edit_myriad_path(myriad) %></td>
        <td><%= link_to 'Destroy', myriad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myriad', new_myriad_path %>
